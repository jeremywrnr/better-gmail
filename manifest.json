{
  "manifest_version": 2,
  "name": "Better Gmail Helper",
  "version": "1.0",
  "description": "Extension for Gmail - Better Navigation",
  "permissions": [
    "*://mail.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["dist/content.bundle.js"],
      "run_at": "document_idle"
    }
  ]
}
