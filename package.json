{
  "name": "gmail-extension",
  "version": "1.0.0",
  "scripts": {
    "start": "web-ext run",
    "build": "webpack",
    "check": "prettier . --check",
    "fix": "prettier . --write",
    "clean": "rm -rf  dist/ web-ext-artifacts/",
    "publish": "webpack --no-watch --mode production && web-ext build --overwrite-dest"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "terser-webpack-plugin": "^5.3.11",
    "web-ext": "^8.3.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
